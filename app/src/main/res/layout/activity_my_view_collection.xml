<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_my_view_collection"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ln.view.customView.MyViewCollection">

    <include layout="@layout/app_title"/>

    <TextView
        android:id="@+id/basic_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/item_press"
        android:padding="8dp"
        android:text="BasicView"
        android:textSize="16sp"
        />

    <TextView
        android:id="@+id/guaguaka"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@drawable/item_press"
        android:padding="8dp"
        android:text="刮刮奖"
        android:textSize="16sp"
        />
    <TextView
        android:id="@+id/progress"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@drawable/item_press"
        android:padding="8dp"
        android:text="进度条"
        android:textSize="16sp"
        />
</LinearLayout>
